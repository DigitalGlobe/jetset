{
  "presets": ["es2015", "react", "stage-1" ],
  "plugins": [
    "transform-decorators-legacy"
  ],
  "env": {
    "es": {
      "presets": [["es2015", {"modules": false}], "react", "stage-1"],
      "plugins": ["transform-decorators-legacy"]
    },
    "umd": {
